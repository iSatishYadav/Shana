@model IEnumerable<Shana.Models.Instargam.Actions>

@{
    ViewBag.Title = "Actions";
}

<h2>Actions</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Go", item.Key)
            </td>
        </tr>
    }

</table>
